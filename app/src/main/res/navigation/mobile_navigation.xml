<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.asdadvance.ui.main.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_mybooking"
        android:name="com.example.asdadvance.ui.main.mybooking.MybookingFragment"
        android:label="my booking"
        tools:layout="@layout/fragment_mybooking" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.asdadvance.ui.main.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.asdadvance.ui.main.notifications.NotificationsFragment"
        android:label="profile"
        tools:layout="@layout/fragment_notifications" />
</navigation>